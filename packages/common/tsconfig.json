{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "isolatedModules": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true
  },
  "exclude": ["node_modules", "dist"],
  "include": ["**/*.ts"]
}
