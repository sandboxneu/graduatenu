{
  "compilerOptions": {
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "target": "es2017",
    "module": "commonjs",
    "strict": true,
    "isolatedModules": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "dist"],
  "include": ["**/*.ts"]
}
