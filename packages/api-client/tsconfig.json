{
  "compilerOptions": {
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "target": "es5",
    "module": "esnext",
    "strict": true,
    "isolatedModules": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "exclude": ["node_modules", "dist"],
  "include": ["**/*.ts"]
}
